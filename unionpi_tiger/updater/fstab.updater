#<src> <mnt_point> <type> <mnt_flags and options> <fs_mgr_flags>
/dev/block/system /system ext4 ro,barrier=1 wait
/dev/block/vendor /vendor ext4 ro,barrier=1 wait
/dev/block/data /data ext4 nosuid,nodev,noatime,barrier=1,data=ordered,noauto_da_alloc wait,reservedsize=104857600
/dev/block/misc /misc none none wait
/dev/block/mmcblk1p1 /sdcard ext4 rw wait
